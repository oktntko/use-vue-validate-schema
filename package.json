{
  "name": "use-vue-validate-schema",
  "version": "1.0.0",
  "description": "Use Vue As Is, Simple Form Validation",
  "keywords": [
    "validate",
    "vue"
  ],
  "homepage": "https://github.com/oktntko/use-vue-validate-schema",
  "bugs": "https://github.com/oktntko/use-vue-validate-schema/issues",
  "license": "MIT",
  "author": "oktntko@gmail.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/oktntko/use-vue-validate-schema"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./valibot": {
      "types": "./dist/valibot/index.d.ts",
      "import": "./dist/valibot/index.es.js",
      "require": "./dist/valibot/index.cjs.js"
    },
    "./zod": {
      "types": "./dist/zodV4/index.d.ts",
      "import": "./dist/zodV4/index.es.js",
      "require": "./dist/zodV4/index.cjs.js"
    },
    "./zodV3": {
      "types": "./dist/zodV3/index.d.ts",
      "import": "./dist/zodV3/index.es.js",
      "require": "./dist/zodV3/index.cjs.js"
    },
    "./zodV4": {
      "types": "./dist/zodV4/index.d.ts",
      "import": "./dist/zodV4/index.es.js",
      "require": "./dist/zodV4/index.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build && tsc --project tsconfig.app.json --noEmit false --declaration --emitDeclarationOnly --outDir dist",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write lib",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "microdiff": "^1.5.0"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^9.4.0",
    "@iconify/json": "^2.2.424",
    "@iconify/tailwind4": "^1.2.0",
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/node24": "^24.0.3",
    "@tsconfig/strictest": "^2.0.8",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.6.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "cspell": "^9.4.0",
    "eslint": "^9.39.2",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "valibot": "^1.2.0",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitepress": "^1.6.4",
    "vitepress-plugin-tabs": "^0.7.3",
    "vitest": "^4.0.16",
    "vue": "^3.5.26",
    "vue-tsc": "^3.2.1",
    "zod": "^4.3.4"
  },
  "peerDependencies": {
    "valibot": "^1.1.0",
    "vue": "^3.5.18",
    "zod": "^3.25.76 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "valibot": {
      "optional": true
    },
    "zod": {
      "optional": true
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "vite@<=5.4.19": ">=5.4.20",
      "vite@>=5.2.6 <=5.4.20": ">=5.4.21",
      "js-yaml@>=4.0.0 <4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0",
      "mdast-util-to-hast@>=13.0.0 <13.2.1": ">=13.2.1"
    }
  }
}
